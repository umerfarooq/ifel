<body>
  <div style="margin-bottom: 100px;">
    <% image_url = ("#{Rails.root.to_s}/public/#{@current_user.company.logo.url(:profile_view)}" if @current_user.company.logo.exists?(:profile_view) ) if @current_user.company.present?%>
    <div style="width: 30%; float: left;"><img src="<%= image_url %>" /></div>
    <div style="width: 60%; float: left;text-align: center; font-size: 20px; font-weight: bold;"><%=  @current_user.business_idea %></div>
    <div style="text-align: right;"><%= Date.today.to_s %></div>
  </div>

  <div class="PrntMnDv">
    <div class="PrntMnCntnt">
      <h1>
        <span class="section-info">
          <%= "Section #{@section.name}: #{@section.title}" %>
        </span>  
      </h1>
      <div class="clear"></div>
      <% @section.items.each do |item| %>
        <% if item.user_answer.present? %>
          <div class="liteBxTtl">
            <h2>
              <%= "#{item.sequence_number}. #{item.title}" %>
            </h2>
            <%= item.user_answer %>
          </div>
        <%end%>
      <%end%>
    </div>
  </div>
</body>
