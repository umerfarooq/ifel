<% form_for @faq_category do |f| %>
  <%= f.error_messages :header_message => nil, :message => nil %>
 
  <p>
    <%= f.label :title %> <br />
    <%= f.text_area :title, :size => "100x10", :class => "adminEditor" %>
  </p>
  <p>
    <%= f.submit :add %>
  </p>
  <p>
    <%= link_to '<< Back', url_for(:back) %>
  </p>
<% end %>
