<h1>Listing news_providers</h1>

<table>
  <tr>
    <th>Logo</th>
    <th>Name</th>
    <th>Url</th>
    <th>Rss</th>
  </tr>

<% @news_providers.each do |news_provider| %>
  <tr>
    <td><%= image_tag news_provider.logo.url(:small) %></td>
    <td><%=h news_provider.name %></td>
    <td><%=h news_provider.url %></td>
    <td><%=h news_provider.rss %></td>
    <td><%= link_to 'Show', news_provider %></td>
    <td><%= link_to 'Edit', edit_news_provider_path(news_provider) %></td>
    <td><%= link_to 'Destroy', news_provider, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New news_provider', new_news_provider_path %>
